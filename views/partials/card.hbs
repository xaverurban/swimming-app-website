<div class="ui card" id ="{{name}}">
  <div class="image">
    <img src="{{image}}">
  </div>
  <div class="content">
    <a class="header">{{fullname}}</a>
    <div class="meta">
      <span class="date">Joined in {{joined}}</span>
    </div>
    <div class="description">
      {{name}} is {{job}} in {{city}}
    </div>
    <div class="map">
      <iframe class="custom-map" src="{{mapUrl}}" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    </div>
  </div>
  <div class="extra content">
    <a>
      <i class="user icon"></i>
      {{mates}} Friends
    </a>
  </div>
</div>